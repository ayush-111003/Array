    //             {
    //                 count++;
    //                 A[j]=-1;
    //             }
    //         }
    //         if(count>1)
    //             cout<<A[i]<<" is appearing "<<count<<" times "<<endl;
    //     }
        
    // }
    
    return 0 ;
}
