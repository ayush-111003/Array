#include<bits/stdc++.h>
using namespace std;

int main()
{   
    int A[10] = {8,3,6,4,6,5,6,8,2,7};
    int H[9] = {0};
    for (int i=0; i < 10; i++)
    {
        H[A[i]]++;
    }
    for (int i = 1; i <=8; i++)
    {
        if(H[i]>1)
            cout<<i<<" is appearing "<<H[i]<<" times "<<endl;
    }
    
    // for (int i = 0; i < 10; i++)
    // {
