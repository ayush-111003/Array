#include<bits/stdc++.h>
using namespace std;

int main()
{   
    int A[10] = {5,8,3,9,6,2,10,7,-1,4};
    int mini = A[0];
    int maxi = A[0];
    for (int i = 0; i <10; i++)
    {
        if(A[i]<mini)
            mini  = A[i];
        else if(A[i]>maxi)
            maxi = A[i];
    }
    cout<<mini<<endl;
    cout<<maxi<<endl;
    //best case is when A = {1,2,3,4,5,6,7,8,12,45,63}
    //worst case is when A = {14,10,,7,6,5,,4,3,2,1}
    return 0 ;
}
