#include<bits/stdc++.h>
using namespace std;
int main()
{//*************************merging two arrays************************************
    int A[5] = {1,3,5,7,9};
    int B[7] = {2,4,6,8,10,11,12};
    int C[12] = {0};
    int i = 0;
    int k = 0;
    int j = 0;
    while (i<5 && j<7)
    {
        if(A[i]<B[j])
            C[k++] = A[i++];
        else
            C[k++] = B[j++];
                        
    } 
    for (; i < 5; i++)
    {
        C[k++] = A[i];
    }
    for (; j < 7; j++)
    {
        C[k++] = B[j];
    }
    for (int out : C)
    {
        cout<<out<<endl;
    }
    
   return 0 ;
}
